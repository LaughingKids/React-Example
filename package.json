{
  "name": "goldmate-exchange-react-frontend",
  "version": "1.0.0",
  "description": "Based on wp-rest api to built a single page wordpress application with react.",
  "main": "index.js",
  "scripts": {
    "webpack-watch": "webpack -w",
    "init": "node ./server/server.js",
    "start": "pm2 start ./server/server.js --name goldmate-exchange",
    "stop": "pm2 stop goldmate-exchange",
    "restart": "pm2 restart ./server/server.js --name goldamate-exchange",
    "dev": "concurrent --kill-others \"npm run webpack-watch\" \"npm run init\""
  },
  "keywords": [
    "React",
    "Wordpress"
  ],
  "author": "Thomas Xiaojie Wang<developer.wxj@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-loader": "^6.2.2",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.4.0",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jquery": "^3.1.0",
    "pm2": "^1.1.3",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-fontawesome": "^1.1.0",
    "react-router": "^2.7.0",
    "request": "^2.74.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "xml2js": "^0.4.17"
  }
}
